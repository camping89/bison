@model IList<ProductOverviewModel>
@if (Model.Count > 0)
{
    <div class="best-seller5">
        <h2 class="title18 title-box5">@T("HomePage.Products")</h2>
        <div class="product-slider">
            <div class="wrap-item group-navi" data-navigation="true" data-pagination="false" data-itemscustom="[[0,1],[560,2],[990,3],[1200,4]]">
                @foreach (var item in Model)
                {
                        @await Html.PartialAsync("_ProductBoxFullHeight", item)
                    
                }
                
            </div>
        </div>
    </div>
    <!-- End Best Seller -->
}