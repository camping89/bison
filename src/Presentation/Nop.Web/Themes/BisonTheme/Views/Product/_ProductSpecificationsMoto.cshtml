@model IList<ProductSpecificationModel>
@if (Model.Count > 0)
{
    <div class="product-specs-box">
        @*<div class="title">
                <strong>@T("Products.Specs")</strong>
            </div>*@
        <div class="table-wrapper">
            <ul class="list-none">
                @for (var i = 0; i < Model.Count; i++)
                {
                    var item = Model[i];
                    <li style="padding: 5px 0;">@Html.Raw(item.ValueRaw)</li>
                }
            </ul>

        </div>
    </div>
}